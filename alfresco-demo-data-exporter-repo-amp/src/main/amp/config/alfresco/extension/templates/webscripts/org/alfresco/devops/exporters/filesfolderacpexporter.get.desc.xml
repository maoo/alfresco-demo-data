<webscript>
  <shortname>Export People,Users,Groups</shortname>
  <description>
  <![CDATA[
  Exports an ACP containing the folder]]></description>
  <url>/api/file-folder/export?path={path}&amp;crawlSelf={crawlSelf}&amp;crawlChildNodes={crawlChildNodes}&amp;crawlContent={crawlContent}&amp;crawlAssociations={crawlAssociations}</url>
  <format default="html">argument</format>
  <authentication>admin</authentication>
  <transaction allow="readonly">required</transaction>
  <lifecycle>internal</lifecycle>
  <family>alfresco-demo-data-exporters</family>
</webscript>